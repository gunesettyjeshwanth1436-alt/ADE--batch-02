{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Jeshwanth"
		},
		"AzureDataLakeStorage_01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_01'"
		},
		"LS_AZURE_SQL_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AZURE_SQL_DB'"
		},
		"LS_SQLSERVER_ONPREM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLSERVER_ONPREM'"
		},
		"AzureDataLakeStorage_01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jeshustorageaccount.dfs.core.windows.net/"
		},
		"LS_AZURE_SQL_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "rainbow-servers.database.windows.net"
		},
		"LS_AZURE_SQL_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "rainbow-DB"
		},
		"LS_AZURE_SQL_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rainbow-DB"
		},
		"LS_SQLSERVER_ONPREM_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-FBJ79AN\\SQLEXPRESS"
		},
		"LS_SQLSERVER_ONPREM_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "maan"
		},
		"LS_SQLSERVER_ONPREM_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_SourceFolder": {
			"type": "string",
			"defaultValue": "Raw_data"
		},
		"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_TargetFolder": {
			"type": "string",
			"defaultValue": "usIng_trigger_copied"
		},
		"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_SourceFileName": {
			"type": "string",
			"defaultValue": "weather.csv"
		},
		"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_TargetFileName": {
			"type": "string",
			"defaultValue": "WEATHER"
		},
		"SCHEDULE_TRIGGER_properties_PL_TRIGGERS_parameters_FOLDERPATH": {
			"type": "string",
			"defaultValue": "Raw_data"
		},
		"SCHEDULE_TRIGGER_properties_PL_TRIGGERS_parameters_FILE_NAME": {
			"type": "string",
			"defaultValue": "weather.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Jeshwanth_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_01_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_01_accountKey')]"
					},
					"LS_AZURE_SQL_DB_password": {
						"value": "[parameters('LS_AZURE_SQL_DB_password')]"
					},
					"LS_SQLSERVER_ONPREM_password": {
						"value": "[parameters('LS_SQLSERVER_ONPREM_password')]"
					},
					"AzureDataLakeStorage_01_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_01_properties_typeProperties_url')]"
					},
					"LS_AZURE_SQL_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_AZURE_SQL_DB_properties_typeProperties_server')]"
					},
					"LS_AZURE_SQL_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_AZURE_SQL_DB_properties_typeProperties_database')]"
					},
					"LS_AZURE_SQL_DB_properties_typeProperties_userName": {
						"value": "[parameters('LS_AZURE_SQL_DB_properties_typeProperties_userName')]"
					},
					"LS_SQLSERVER_ONPREM_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLSERVER_ONPREM_properties_typeProperties_server')]"
					},
					"LS_SQLSERVER_ONPREM_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLSERVER_ONPREM_properties_typeProperties_database')]"
					},
					"LS_SQLSERVER_ONPREM_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLSERVER_ONPREM_properties_typeProperties_userName')]"
					},
					"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_SourceFolder": {
						"value": "[parameters('SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_SourceFolder')]"
					},
					"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_TargetFolder": {
						"value": "[parameters('SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_TargetFolder')]"
					},
					"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_SourceFileName": {
						"value": "[parameters('SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_SourceFileName')]"
					},
					"SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_TargetFileName": {
						"value": "[parameters('SCHEDULE_TRIGGER_properties_PL_COPY_CSV_FILE_DYNAMIIC_parameters_TargetFileName')]"
					},
					"SCHEDULE_TRIGGER_properties_PL_TRIGGERS_parameters_FOLDERPATH": {
						"value": "[parameters('SCHEDULE_TRIGGER_properties_PL_TRIGGERS_parameters_FOLDERPATH')]"
					},
					"SCHEDULE_TRIGGER_properties_PL_TRIGGERS_parameters_FILE_NAME": {
						"value": "[parameters('SCHEDULE_TRIGGER_properties_PL_TRIGGERS_parameters_FILE_NAME')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
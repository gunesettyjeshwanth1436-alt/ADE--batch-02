{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Jeshwanth"
		},
		"AzureDataLakeStorage_01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_01'"
		},
		"LS_AZURE_SQL_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AZURE_SQL_DB'"
		},
		"AzureDataLakeStorage_01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jeshustorageaccount.dfs.core.windows.net/"
		},
		"LS_AZURE_SQL_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "rainbow-servers.database.windows.net"
		},
		"LS_AZURE_SQL_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "rainbow-DB"
		},
		"LS_AZURE_SQL_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rainbow-DB"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Jeshwanth_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_01_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_01_accountKey')]"
					},
					"LS_AZURE_SQL_DB_password": {
						"value": "[parameters('LS_AZURE_SQL_DB_password')]"
					},
					"AzureDataLakeStorage_01_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_01_properties_typeProperties_url')]"
					},
					"LS_AZURE_SQL_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_AZURE_SQL_DB_properties_typeProperties_server')]"
					},
					"LS_AZURE_SQL_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_AZURE_SQL_DB_properties_typeProperties_database')]"
					},
					"LS_AZURE_SQL_DB_properties_typeProperties_userName": {
						"value": "[parameters('LS_AZURE_SQL_DB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
{
	"name": "PL_COPY_CSV_FILE_DYNAMIIC",
	"properties": {
		"activities": [
			{
				"name": "CopyFilesDynamically",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				}
			}
		],
		"parameters": {
			"SourceFolder": {
				"type": "string",
				"defaultValue": "raw_folder"
			},
			"TargetFolder": {
				"type": "string",
				"defaultValue": "Arcive_DS"
			},
			"SourceFileName": {
				"type": "string",
				"defaultValue": "employment_status.csv"
			},
			"TargetFileName": {
				"type": "string",
				"defaultValue": "EmploymentFileCopied"
			}
		},
		"folder": {
			"name": "DATA_MIGRATION_TASKS"
		},
		"annotations": []
	}
}